{
	"scripts": {
		"build": "cross-env NODE_ENV=production concurrently \"npm run build:css-app\" \"npm run build:css-editor\" \"npm run build:js\"",
		"dev": "cross-env NODE_ENV=development concurrently \"npm run watch:css-app\" \"npm run watch:css-editor\" \"npm run watch:js\" \"npm run browser-sync\"",
		"watch:js": "esbuild js/index.js --bundle --watch --outfile=static/site.js",
		"build:js": "esbuild js/index.js --bundle --minify --outfile=static/site.js",
		"build:css-app": "postcss ./css/main.css -o ./static/style.css",
		"build:css-editor": "postcss ./css/editor.css -o ./static/editor.css",
		"watch:css-app": "postcss ./css/main.css -o ./static/style.css --watch",
		"watch:css-editor": "postcss ./css/editor.css -o ./static/editor.css --watch",
		"browser-sync": "cross-env NODE_ENV=development browser-sync start --proxy \"third-culture.test\" --host=\"third-culture.test\" --no-inject-changes --files=\"./\""
	},
	"dependencies": {
		"@dgreenheck/three-pinata": "^2.0.1",
		"@swup/head-plugin": "^2.3.1",
		"@swup/preload-plugin": "^3.2.11",
		"@swup/scripts-plugin": "^2.1.0",
		"autoprefixer": "^10.4.17",
		"browser-sync": "^2.29.3",
		"cannon-es": "^0.20.0",
		"concurrently": "^7.6.0",
		"cross-env": "^7.0.3",
		"embla-carousel": "^8.3.0",
		"embla-carousel-auto-scroll": "^8.3.0",
		"esbuild": "^0.15.18",
		"gsap": "^3.13.0",
		"lenis": "^1.3.8",
		"postcss": "^8.4.35",
		"postcss-cli": "^10.1.0",
		"postcss-import": "^15.1.0",
		"postcss-nested": "^6.0.1",
		"swiper": "^10.3.1",
		"swup": "^4.8.2",
		"three": "^0.179.1",
		"three-text": "^0.2.16",
		"troika-three-text": "^0.52.4"
	},
	"devDependencies": {
		"@gltf-transform/core": "^4.2.1",
		"@gltf-transform/extensions": "^4.2.1",
		"@gltf-transform/functions": "^4.2.1",
		"@tailwindcss/typography": "^0.5.10",
		"cssnano": "^5.1.15",
		"draco3dgltf": "^1.5.7",
		"esbuild-serve": "^1.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-twig-melody": "^0.4.6",
		"tailwindcss": "^3.4.1"
	},
	"type": "module"
}
